{
  "expo": {
    "name": "chatbox",
    "slug": "chatbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/chatbox_svg_logo.svg",
      "resizeMode": "contain",
      "backgroundColor": "#113833"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.chatbox"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.chatbox"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "86bd3c34-4655-4f4b-9a57-77d620d4ee37"
      }
    }
  }
}
